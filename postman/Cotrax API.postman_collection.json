		{
			"name": "Send Time Logs",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Dapatkan waktu saat ini\r",
							"let startTime = new Date();\r",
							"\r",
							"// Buat waktu untuk 1 jam di masa depan dari sekarang\r",
							"let futureTime = new Date();\r",
							"futureTime.setHours(startTime.getHours() + 1);\r",
							"\r",
							"// Simpan kedua waktu tersebut sebagai environment variable di Postman\r",
							"// .toISOString() akan mengubahnya menjadi format yang bisa diterima Go\r",
							"pm.environment.set(\"start_time_future\", startTime.toISOString());\r",
							"pm.environment.set(\"end_time_future\", futureTime.toISOString());"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "noauth"
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"file_id\": 1,\r\n    \"start_time\": \"{{start_time_future}}\",\r\n    \"end_time\": \"{{end_time_future}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": "http://localhost:7000/api/v1/time_logs"
			},
			"response": []
		},
